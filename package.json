{
  "name": "printbridge",
  "version": "0.1.0",
  "private": true,
  "description": "Web-to-Print module for converting web content to high-quality PDFs",
  "workspaces": [
    "packages/*",
    "apps/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "keywords": [
    "web-to-print",
    "pdf",
    "print",
    "layout",
    "exhibition"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.1.0",
    "turbo": "^2.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
